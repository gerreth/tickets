<header role="header">
  <%= link("Start", to: "/") %>
  <%= if logged_in?(@conn) do %>
    Hello <%= @current_user.username %>
    <%= link("Tickets", to: "/tickets") %>
    <%= link("Users", to: "/users") %>
    <%= link "Logout", to: session_path(@conn, :delete), method: :delete %>
  <% else %>
    <%= link("Login", to: "/login") %>
  <% end %>
</header>
